{
  "metadata": [
    {
      "src": [
        { "src": "../src", "files": [ "**.csproj" ], "exclude": [ "**/bin/**", "**/obj/**" ] }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      { "files": [ "api/**.yml", "api/index.md" ] },
      { "files": [ "articles/**.md", "articles/**/toc.yml", "toc.yml", "*.md" ] }
    ],
    "resource": [
      { "src": "../graphics/docs", "files": [ "*" ] }
    ],
    "overwrite": [
      { "files": [ "apidoc/**.md" ], "exclude": [ "obj/**", "_site/**" ] }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appName": "AaaLib",
      "_appTitle": "AaaLib documentation",
      "_appFooter": "<span>Copyright Â© Tenacom and <a href=\"https://github.com/Tenacom/AaaLib/#contributors\" target=\"_new\">contributors</a>.<br>Licensed under <a href=\"https://github.com/Tenacom/AaaLib/blob/main/LICENSE\" target=\"_new\">MIT license</a></span>",
      "_gitContribute": { "branch": "main" }
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [ "default", "templates/singulinkfx", "plugins/memberpage.2.59.4/content" ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ]
  }
}
